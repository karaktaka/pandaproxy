# PandaProxy environment configuration
# Copy this file to .env and fill in your values

# Required: IP address of your BambuLab printer
PRINTER_IP=192.168.1.100

# Required: Access code from your printer (Settings > Network > Access Code)
ACCESS_CODE=12345678

# Required: Serial number of your printer (Settings > Network > Serial Number)
SERIAL_NUMBER=01A12B2C

# Address to bind the proxy server to (default: 0.0.0.0)
BIND_ADDRESS=0.0.0.0

# Services to enable (comma-separated list, default: camera)
SERVICES=""

# Optional: Enable all services (overrides SERVICES variable)
ENABLE_ALL=False
